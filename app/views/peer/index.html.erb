<h1>Please provide a zipcode or update the data file</h1>
<br/>
<p>Update CBSA to MSA file: </p>
<%= form_tag({action: :upload}, multipart: true) do %>
  <%= file_field_tag 'csv' %>
  <%= submit_tag("Update") %>
<% end %>
<hr/>
<%= form_tag({action: :search}, method: "get") do %>
  <%= label_tag(:q, "Search by Zipcode:") %>
  <%= text_field_tag(:zip) %>
  <%= submit_tag("Search") %>
<% end %>
